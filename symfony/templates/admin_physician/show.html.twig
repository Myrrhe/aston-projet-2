{% extends 'admin/base_admin.html.twig' %}

{% block title %}Physician{% endblock %}

{% block body %}
    <h1>Physician</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ physician.id }}</td>
            </tr>
            <tr>
                <th>Ssn</th>
                <td>{{ physician.ssn }}</td>
            </tr>
            <tr>
                <th>FirstName</th>
                <td>{{ physician.firstName }}</td>
            </tr>
            <tr>
                <th>LastName</th>
                <td>{{ physician.lastName }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ physician.email }}</td>
            </tr>
            <tr>
                <th>PhoneNumber</th>
                <td>{{ physician.phoneNumber }}</td>
            </tr>
            <tr>
                <th>LicenseNumber</th>
                <td>{{ physician.licenseNumber }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_physician_index') }}">back to list</a>

    <a href="{{ path('app_admin_physician_edit', {'id': physician.id}) }}">edit</a>

    {{ include('admin_physician/_delete_form.html.twig') }}
{% endblock %}
