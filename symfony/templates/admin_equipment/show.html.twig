{% extends 'admin/base_admin.html.twig' %}

{% block title %}Equipment{% endblock %}

{% block body %}
    <h1>Equipment</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ equipment.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ equipment.name }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ equipment.type }}</td>
            </tr>
            <tr>
                <th>TotalQuantity</th>
                <td>{{ equipment.totalQuantity }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_equipment_index') }}">back to list</a>

    <a href="{{ path('app_admin_equipment_edit', {'id': equipment.id}) }}">edit</a>

    {{ include('admin_equipment/_delete_form.html.twig') }}
{% endblock %}
